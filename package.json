{
  "name": "app-client",
  "productName": "app-client",
  "version": "1.0.2",
  "description": "TechnoFrance APP",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SashokNekulin/app-client.git"
  },
  "keywords": [],
  "author": {
    "name": "TechnoFrance",
    "email": "support@techno-france.ru"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/tf.icon",
        "nameShort": "ТехноФранц",
        "nameLong": "ТехноФранц Windows APP",
        "applicationName": "технофранц",
        "win32NameVersion": "ТехноФранц Windows APP",
        "win32DirName": "ТехноФранц Windows APP",
        "win32SetupExeBasename": "TechnoFranceSetup",
        "win32AppUserModelId": "TechnoFrance.ClientApp",
        "win32ShellNameShort": "ТехноФранц",
        "win32MutexName": "technofrance",
        "win32RegValueName": "TechnoFrance"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "app_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "electron": "17.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
